{{- $.Scratch.Add "index" slice -}}
{{- range first 10 site.RegularPages -}}
{{- $date:= time.Format ":date_long" .PublishDate -}}
{{- $.Scratch.Add "index" (dict "title" .Title "date" $date "thumbnail" .Params.thumbnail "description" .Params.description "tags" .Params.tags "contents" .Plain "permalink" .RelPermalink) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
